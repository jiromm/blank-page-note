{
	"name": "Blank Page Note",
	"description": "To take notes directly from chrome's blank page",
	"manifest_version": 2,
	"version": "0.1",
	"incognito": "split",

	"chrome_url_overrides": {
		"newtab": "blank.html"
	},

	"content_scripts": [
		{
			"matches": ["http://www.google.com/*"],
			"js": [
				"js/vue.js",
				"js/marked.js",
				"js/main.js"
			]
		}
	]
}
